{
  "name": "proyecto",
  "private": true,
  "workspaces": [
    "server",
    "client"
  ],
  "scripts": {
    "dev": "npm-run-all -p dev:server dev:client",
    "dev:server": "npm --workspace server run dev",
    "dev:client": "npm --workspace client run start",
    "start": "npm-run-all -p start:server start:client",
    "start:server": "npm --workspace server run start",
    "start:client": "npm --workspace client run start"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}




